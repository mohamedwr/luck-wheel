{
  "include": ["./static/wheel.ts"],
  "compilerOptions": {
    "module": "ES6",
    "removeComments": true,
    "target": "ES2015",
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": false,
    "preserveConstEnums": false,
    "exactOptionalPropertyTypes": true,
    "alwaysStrict": true
  }
}
